<Window x:Class="LiveSplit.OriAndTheBlindForest.OriHitboxDisplay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:local="clr-namespace:LiveSplit.OriAndTheBlindForest"
        Title="Ori Viewer" Height="800" Width="800" Background="{x:Null}" ResizeMode="NoResize"
        ShowInTaskbar="False" AllowsTransparency="True"
        WindowStyle="None" Topmost="True">
	<Grid>
		<Canvas Name="CanvasInfo" HorizontalAlignment="Left" Height="300" VerticalAlignment="Top" Width="300">
			<local:OutlinedTextBlock x:Name="OriInfo" NormalText="true" Visibility="Hidden" Fill="White" FontSize="14" Canvas.Left="206" Canvas.Top="54"></local:OutlinedTextBlock>
		</Canvas>
	</Grid>
</Window>
